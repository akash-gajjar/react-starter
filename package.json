{
  "name": "react-starter",
  "description": "React frontend starter template.",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "Aakash Gajjar",
    "email": "me@aakashgajjar.dev",
    "url": "https://aakashgajjar.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/akash-gajjar/react-starter#readme",
  "repository": "git@github.com:akash-gajjar/react-starter.git",
  "main": "./src/index.js",
  "scripts": {
    "build:dev": "NODE_ENV=production webpack --env env=production --mode production --config=webpack.config.prod.js",
    "build:prod": "NODE_ENV=production webpack --env env=production --mode production --config=webpack.config.prod.js",
    "lint:fix": "eslint --fix . --ext .js,.jsx",
    "lint:script": "eslint . --ext .js,.jsx",
    "lint:style": "stylelint './src/**/*.js'",
    "prettier": "prettier -c -w .",
    "start": "NODE_OPTIONS=--max_old_space_size=2048 NODE_ENV=development webpack-dev-server --env env=development --mode development --config=webpack.config.dev.js --open --progress",
    "serve": "serve -p 6661 -s dist/",
    "postinstall": "yarn install"
  },
  "dependencies": {
